<h1>Edit <%= @song.name %></h1>

<%= form_for(@song) do |f| %>
    <%= f.label :name %>
    <br>
    <%= f.text_field :name %>
    <br>
    <br>
    <%= f.label :artist %>
    <br>
    <%= f.text_field :artist, value: @song.artist.name %>
    <br>
    <br>
    <%= f.label :genre %>
    <br>
    <%= f.text_field :genre, value: @song.genre.name %>
    <br>
    <br>
    <%= f.submit 'Submit My Changed Banger' %>
<% end %>