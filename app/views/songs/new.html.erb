<h1>Your New Song</h1>
<%= form_for(@song) do |f| %>
    <%= f.label :name %>
    <br>
    <%= f.text_field :name %>
    <br>
    <br>
    <%= f.label :artist %>
    <br>
    <%= f.text_field :artist %>
    <br>
    <br>
    <%= f.label :genre %>
    <br>
    <%= f.text_field :genre %>
    <br>
    <br>
    <%= f.submit 'Submit My New Banger' %>
 <% end %>